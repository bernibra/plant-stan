  file = {/Users/bemora/Software/Zotero/storage/W2A9AUVY/Kearney and Porter - 2009 - Mechanistic niche modelling combining physiologic.pdf;/Users/bemora/Software/Zotero/storage/KPW2Z78T/j.1461-0248.2008.01277.html},
  journal = {Ecology Letters},
  keywords = {Biophysical ecology,climate,fundamental niche,geographical information systems,microclimate,physiological ecology,realized niche,species distribution modelling},
