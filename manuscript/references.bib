  keywords = {Animals,Anthozoa,Biodiversity,Dissimilarity,Ecology,Fishes,Heterogeneity,Levene's test,Multivariate analysis,Multivariate Analysis,Permutation tests,Principal coordinates,Robust methods,Sparrows},
  language = {eng},
