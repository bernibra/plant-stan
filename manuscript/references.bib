@article{montesinos-navarroCommunityStructureInforms2018,
  title = {Community Structure Informs Species Geographic Distributions},
  author = {{Montesinos-Navarro}, Alicia and Estrada, Alba and Font, Xavier and Matias, Miguel G. and Meireles, Catarina and Mendoza, Manuel and Honrado, Joao P. and Prasad, Hari D. and Vicente, Joana R. and Early, Regan},
