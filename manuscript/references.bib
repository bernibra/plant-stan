}

@article{jordanoPatternsMutualisticInteractions1987,
  title = {Patterns of {{Mutualistic Interactions}} in {{Pollination}} and {{Seed Dispersal}}: {{Connectance}}, {{Dependence Asymmetries}}, and {{Coevolution}}},
