@article{poggiatoInterpretationsJointModeling2021,
  title = {On the {{Interpretations}} of {{Joint Modeling}} in {{Community Ecology}}},
  author = {Poggiato, Giovanni and M{\"u}nkem{\"u}ller, Tamara and Bystrova, Daria and Arbel, Julyan and Clark, James S. and Thuiller, Wilfried},
  year = {2021},
