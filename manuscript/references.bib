@article{chomickiMacroevolutionaryAssemblyAnt2015,
  title = {Macroevolutionary Assembly of Ant/Plant Symbioses: {{Pseudomyrmex}} Ants and Their Ant-Housing Plants in the {{Neotropics}}},
  shorttitle = {Macroevolutionary Assembly of Ant/Plant Symbioses},
  author = {Chomicki, Guillaume and Ward, Philip S. and Renner, Susanne S.},
  year = {2015},
  month = nov,
  volume = {282},
