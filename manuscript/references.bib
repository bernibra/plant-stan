  journal = {arXiv:1908.09440 [nlin, physics:physics, stat]},
  keywords = {Computer Science - Social and Information Networks,Mathematics - Statistics Theory,Nonlinear Sciences - Adaptation and Self-Organizing Systems,Physics - Physics and Society,Statistics - Applications},
