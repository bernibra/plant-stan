@article{lustenhouwerTraitbasedUnderstandingWood2020,
  title = {A Trait-Based Understanding of Wood Decomposition by Fungi},
  author = {Lustenhouwer, Nicky and Maynard, Daniel S. and Bradford, Mark A. and Lindner, Daniel L. and Oberle, Brad and Zanne, Amy E. and Crowther, Thomas W.},
  year = {2020},
